<mvc:View controllerName="Zrt006.wallet.create.ZRT_006_WALLET_CREATE.controller.Home" xmlns:html="http://www.w3.org/1999/xhtml"                                                                                                                                
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" xmlns="sap.m" xmlns:smartForm="sap.ui.comp.smartform"                                                                                                                                                         
	xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1" xmlns:l="sap.ui.layout"                                                                                                                                                          
	xmlns:smartField="sap.ui.comp.smartfield">                                                                                                                                                                                                                    
	<Page>                                                                                                                                                                                                                                                        
		<content>                                                                                                                                                                                                                                                    
			<smartForm:SmartForm title="{i18n>FormTitle}" id="idWalletForm" app:defaultCheckBoxDisplayBehaviour="OnOff"                                                                                                                                                 
				app:defaultDropDownDisplayBehaviour="idAndDescription" editable="true" editTogglable="true">                                                                                                                                                               
				<smartForm:customToolbar>                                                                                                                                                                                                                                  
					<OverflowToolbar>                                                                                                                                                                                                                                         
						<ToolbarSpacer/>                                                                                                                                                                                                                                         
						<Button press="onCreate" type="Transparent" text="Save" icon="sap-icon://visits"/>                                                                                                                                                                       
					</OverflowToolbar>                                                                                                                                                                                                                                        
				</smartForm:customToolbar>                                                                                                                                                                                                                                 
				<smartForm:layout>                                                                                                                                                                                                                                         
					<smartForm:Layout labelSpanXL="2" labelSpanL="2" labelSpanM="2" labelSpanS="2" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="4"                                                                                                               
						columnsXL="2" columnsL="2" columnsM="2"/>                                                                                                                                                                                                                
				</smartForm:layout>                                                                                                                                                                                                                                        
				<smartForm:Group>                                                                                                                                                                                                                                          
					<smartForm:layoutData>                                                                                                                                                                                                                                    
						<l:GridData span="XL8 L8 M8 S8"/>                                                                                                                                                                                                                        
					</smartForm:layoutData>                                                                                                                                                                                                                                   
					<!--<smartForm:GroupElement>-->                                                                                                                                                                                                                           
					<!--	<smartField:SmartField editable="true" value="{TransactionNo}">-->                                                                                                                                                                                   
					<!--		<smartField:configuration>-->                                                                                                                                                                                                                       
					<!--			<smartField:Configuration displayBehaviour="descriptionOnly" controlType="input"/>-->                                                                                                                                                              
					<!--		</smartField:configuration>-->                                                                                                                                                                                                                      
					<!--	</smartField:SmartField>-->                                                                                                                                                                                                                          
					<!--</smartForm:GroupElement>-->                                                                                                                                                                                                                          
					<smartForm:GroupElement>                                                                                                                                                                                                                                  
						<smartField:SmartField editable="true" value="{TransactionType}" change='onTransactionNoChange'>                                                                                                                                                         
							<smartField:configuration>                                                                                                                                                                                                                              
								<smartField:Configuration displayBehaviour="descriptionOnly" controlType="dropDownList"/>                                                                                                                                                              
							</smartField:configuration>                                                                                                                                                                                                                             
						</smartField:SmartField>                                                                                                                                                                                                                                 
					</smartForm:GroupElement>                                                                                                                                                                                                                                 
					<smartForm:GroupElement>                                                                                                                                                                                                                                  
						<smartField:SmartField editable="true" value="{DealerId}" >                                                                                                                                                                                              
							<smartField:configuration>                                                                                                                                                                                                                              
								<smartField:Configuration displayBehaviour="descriptionOnly"/>                                                                                                                                                                                         
							</smartField:configuration>                                                                                                                                                                                                                             
						</smartField:SmartField>                                                                                                                                                                                                                                 
					</smartForm:GroupElement>                                                                                                                                                                                                                                 
					<smartForm:GroupElement id="idAmount">                                                                                                                                                                                                                    
						<smartField:SmartField editable="true" value="{Amount}" uomEditable="false" >                                                                                                                                                                            
							<smartField:configuration>                                                                                                                                                                                                                              
								<smartField:Configuration displayBehaviour="descriptionOnly"/>                                                                                                                                                                                         
							</smartField:configuration>                                                                                                                                                                                                                             
						</smartField:SmartField>                                                                                                                                                                                                                                 
					</smartForm:GroupElement>                                                                                                                                                                                                                                 
					<smartForm:GroupElement>                                                                                                                                                                                                                                  
						<smartField:SmartField editable="true" value="{TransactionDate}">                                                                                                                                                                                        
							<smartField:configuration>                                                                                                                                                                                                                              
								<smartField:Configuration displayBehaviour="descriptionOnly"/>                                                                                                                                                                                         
							</smartField:configuration>                                                                                                                                                                                                                             
						</smartField:SmartField>                                                                                                                                                                                                                                 
					</smartForm:GroupElement>                                                                                                                                                                                                                                 
					<smartForm:GroupElement>                                                                                                                                                                                                                                  
						<smartField:SmartField editable="true" value="{SlipNumber}" id="idSlipNumber">                                                                                                                                                                           
							<smartField:configuration >                                                                                                                                                                                                                             
								<smartField:Configuration displayBehaviour="descriptionOnly"/>                                                                                                                                                                                         
							</smartField:configuration>                                                                                                                                                                                                                             
						</smartField:SmartField>                                                                                                                                                                                                                                 
					</smartForm:GroupElement>                                                                                                                                                                                                                                 
					<smartForm:GroupElement id="idSign">                                                                                                                                                                                                                      
						<smartField:SmartField editable="true" value="{Sign}" change='onSignChange'>                                                                                                                                                                             
							<smartField:configuration>                                                                                                                                                                                                                              
								<smartField:Configuration displayBehaviour="descriptionOnly" controlType="dropDownList"/>                                                                                                                                                              
							</smartField:configuration>                                                                                                                                                                                                                             
						</smartField:SmartField>                                                                                                                                                                                                                                 
					</smartForm:GroupElement>                                                                                                                                                                                                                                 
				</smartForm:Group>                                                                                                                                                                                                                                         
			</smartForm:SmartForm>                                                                                                                                                                                                                                      
		</content>                                                                                                                                                                                                                                                   
		<footer>                                                                                                                                                                                                                                                     
			<OverflowToolbar id="otbFooter">                                                                                                                                                                                                                            
				<ToolbarSpacer/>                                                                                                                                                                                                                                           
				<!--<Button id="messagePopoverButton" icon="sap-icon://message-popup" type="Emphasized" text="{= ${message>/}.length }"-->                                                                                                                                 
				<!--	visible="{= !!${message>/}.length }" press=".onOpenMessages"/>-->                                                                                                                                                                                     
			</OverflowToolbar>                                                                                                                                                                                                                                          
		</footer>                                                                                                                                                                                                                                                    
	</Page>                                                                                                                                                                                                                                                       
</mvc:View>                                                                                                                                                                                                                                                    